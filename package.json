{
  "name": "grapesjs-plugin-tinymce6",
  "version": "0.1.2",
  "description": "Replace the built-in RTE with Tinymce",
  "main": "dist/grapesjs-plugin-tinymce6",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoodPHP/grapesjs-plugin-tinymce6.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "tinydocs",
    "wysiwyg"
  ],
  "author": "DevFuture",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.1.1",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "tinymce": "^6.2.0"
  }
}
